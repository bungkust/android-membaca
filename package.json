{
  "name": "membaca-tk-satu",
  "private": true,
  "version": "1.5.9",
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "dev:mobile": "turbo dev --filter=mobile",
    "build": "turbo build",
    "build:web": "turbo build --filter=web",
    "build:mobile": "turbo build --filter=mobile",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules packages/*/node_modules",
    "preview": "cd packages/web && vite preview"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "eslint": "^9.32.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "turbo": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
